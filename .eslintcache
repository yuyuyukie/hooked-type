tUser]);\r\n\r\n  if (!props.isVisible) {\r\n    return <div className=\"favoriteMoviesContainer\" />;\r\n  }\r\n  if (!currentUser) {\r\n    return (\r\n      <div className=\"notLoggedInMessage\" style={{ fontSize: \"1.5rem\" }}>\r\n        We need you to \"Sign in\" to use this functionality.\r\n      </div>\r\n    );\r\n  }\r\n  const createFavMovieDiv = (movies: MovieObject[], loading: boolean) => {\r\n    movies.forEach((movie) => {\r\n      movie.favorite = true;\r\n    });\r\n    if (loading) {\r\n      return <span>loading...</span>;\r\n    }\r\n    return movieDivFactory(movies);\r\n  };\r\n  return (\r\n    <React.Fragment>\r\n      <div className=\"toolbox\" />\r\n      <div className=\"favoriteMoviesContainer\">\r\n        {createFavMovieDiv(favMovies, loading)}\r\n      </div>\r\n    </React.Fragment>\r\n  );\r\n};\r\nexport default FavoriteMode;\r\n","C:\\Users\\zx275\\Documents\\WebApplications\\hooked-type\\src\\components\\AppContext.tsx",[],"C:\\Users\\zx275\\Documents\\WebApplications\\hooked-type\\src\\components\\AuthProvider.tsx",["90","91"],"C:\\Users\\zx275\\Documents\\WebApplications\\hooked-type\\src\\components\\SearchMode.tsx",[],"C:\\Users\\zx275\\Documents\\WebApplications\\hooked-type\\src\\components\\Main.tsx",["92","93"],"import { useState } from \"react\";\r\nimport { Mode, MovieObject } from \"./App\";\r\nimport FavoriteMode from \"./FavoriteMode\";\r\nimport SearchMode from \"./SearchMode\";\r\n\r\ntype Props = {\r\n  mode: Mode;\r\n};\r\nconst Main: React.FC<Props> = (props) => {\r\n  // const [searchMovies, setSearchMovies] = useState<MovieObject[]>([]);\r\n  // const [favoriteMovies, setFavoriteMovies] = useState<MovieObject[]>([]);\r\n\r\n  return (\r\n    <>\r\n      <SearchMode isVisible={props.mode === Mode.Search} />\r\n      <FavoriteMode isVisible={props.mode === Mode.Favorite} />\r\n    </>\r\n  );\r\n};\r\nexport default Main;\r\n",{"ruleId":"94","severity":1,"message":"95","line":1,"column":17,"nodeType":"96","messa